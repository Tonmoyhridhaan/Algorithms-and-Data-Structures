#include<bits/stdc++.h>
using namespace std;
#define sf scanf
#define pf printf
#define pb push_back
#define all(a) a.begin(),a.end()
#define i64 long long int
const double pi = 2*acos(0) ;
const int maxn = (i64)2e5+5;

vector<int>v;
const int N = 1000000;  //highest number er sqrt(n)+2 porjonto ber korte hobe
int pr[N+1],k=1;
int lp[N+1];
int sieve()
{
    for (int i=2; i<=N; ++i)
    {
        if (lp[i] == 0)
        {
            lp[i] = i;
            pr[k++] = i;
        }
        for (int j=0; j<k && pr[j]<=lp[i] && i*pr[j]<=N; ++j)
            lp[i * pr[j]] = pr[j];
    }
return 0;
}

int prime_fact(int n)
{
    int i;
    for(i = 1 ; pr[i]*pr[i]<=n; i++)
    {
        while(n%pr[i] == 0)
        {
            v.pb(pr[i]);
            n /= pr[i];
        }
    }
    if(n>1) v.pb(n);
}
int main()
{
    int n;
    cin>>n;
    sieve();
    prime_fact(n);
    int sz = v.size();
    for(int i = 0 ; i<sz ; i++)
    {
        cout<<v[i]<<" ";;
    }

return 0;
}
